<!-- Start Banner Carousel -->
<div class="banner-outer">
    <div class="banner-slider">
        <div class="slide1" style="background: url(../../imageshome/banner1.jpg)">
            <div class="container">
                <div class="content animated fadeInRight">
                    <div class="fl-right">
                        <h1 class="animated fadeInRight">5★<span class="animated fadeInRight">EDUCATION QUALITY</span> </h1>
                        <p class="animated fadeInRight">FPT University</p>
