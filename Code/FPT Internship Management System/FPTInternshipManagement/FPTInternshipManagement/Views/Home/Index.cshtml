        </ul>
    </div>

</section>
<!-- End About Section -->
<!-- Start Cources Section -->
<section class="our-cources padding-lg" style="display: block;
    background: url(../../imageshome/cources-bg.jpg) no-repeat center top / cover;
    position: relative;">
    <div class="container">
        <h2> Top Recruiter</h2>
        <ul class="course-list owl-carousel">

            @{
                var ListRecruiter = ViewBag.TopRecruiter;
                var Location = ViewBag.TopLocation;
                var Image = ViewBag.TopImage;
                foreach (var user in ListRecruiter)
                {
                    <li style="display: block;background: url() no-repeat center top;position: relative;">
                        <div class="inner">
                            <figure><img src="@Url.Content(Image[user.Key])" alt=""></figure>

                            <div class="bottom-txt clearfix">
                                <div class="duration">
                                    <h4>@user.Key.Name</h4>
                                    @*<span>@user.Value Job - @Location[user.Key.LocationID] </span>*@
                                    <span>@user.Value Job  </span>
                                </div>

                            </div>
                        </div>
                    </li>
                }
            }

        </ul>
    </div>
</section>
