<style>
	.search-box {
		width: 300px !important;
		height: 42px !important;
	}

	.search-horizontal select {
		opacity: 1;
	}

	.chosen {
		width: 100%;
		height: 100%;
		border-radius: 3px;
	}
</style>
<!-- container for search SV -->
<div id="uni_container">
	<div class="s-subpage">
		<div id="NewSearchJob">
			<form>
				<div class="search-horizontal" style="position:static" id="box_search_resume">
					<div class="search_keyword">
						<label>Từ khóa </label>
						<div id="tip_search_exp">
							<label>(Hướng dẫn tìm kiếm)</label>
						</div>
					</div>
					<input type="text" name="keyword" id="keyword"
						   class="width_360 CBTextInput fl_left box_multiSelect_location search-box"
						   autocomplete='off'>
					<a href=" javascript:void(0);" class="closex" id="close_keyword"></a>
					<div class="fl_left box_multiSelect_industry">
						<select name="list_industries[]" id="industry" class="chosen"
								data-placeholder="Tất cả ngành nghề" size="1">
							<output label="Công Nghệ Thông Tin" class="option-title">
								<option value="cong-nghe-thong-tin">Công Nghệ Thông Tin</option>
								<option value="IoT" class="option">IoT</option>
								<option value="tri-tue-nhan-tao" class="option">Trí Tuệ Nhân Tạo</option>
								<option value=" ky-thuat-phan-mem" class="option">Kỹ Thuật Phần Mềm</option>
								<option value=" he-thong-thong-tin" class="option">Hệ Thống Thông Tin</option>
								<option value="an-toan-thong-tin" class="option">An Toàn Thông Tin</option>
								<option value=" thiet-ke-my-thuat-so" class="option">Thiết Kế Mỹ Thuật Số</option>
							</output>
							<output label="Quản Trị Kinh Doanh" class="option-title">
								<option value="digital-marketing" class="option">Digital Marketing</option>
								<option value="kinh-doanh-quoc-te" class="option">Kinh Doanh Quốc Tế</option>
								<option value="quan-tri-khach-san" class="option">Quản Trị Khách Sạn</option>
								<option value="quan-tri-du-lich-lu-hanh" class="option">Quản Trị Du Lịch Và Lữ Hành</option>
								<option value="quan-ly-truyen-thong-da-phuong-tien" class="option">
									Quản Lý Truyền Thông Đa Phương
									Tiện
								</option>
							</output>
							<output label="Ngôn Ngữ Học" class="option-title">
								<option value="ngon-ngu-anh" class="option">Ngôn Ngữ Anh</option>
								<option value="ngon-ngu-nhat" class="option">Ngôn Ngữ Nhật</option>
								<option value="ngon-ngu-han-quoc" class="option">Ngôn Ngữ Hàn Quốc</option>
							</output>
						</select>
					</div>
					<div class="fl_left box_multiSelect_location">
						<select name="list_location[]" id="location" class="chosen"
								data-placeholder="Tất cả địa điểm" size="1">
							<optgroup label="Việt Nam" class="option-title">
								<option value="ha-noi">Hà Nội</option>
								<option value="ho-chi-minh">Hồ Chí Minh</option>
								<option value="da-nang">Đà Nẵng</option>
							</optgroup>
						</select>
					</div>
					<input type="submit" class="searchvt1  BtnButton" value="Tìm">
					<div id="option_keyword" class="search_option" style="display:none;">
						<span>Tìm</span>
					</div>
				</div>
			</form>
		</div>
	</div>
</div>
    </select>

