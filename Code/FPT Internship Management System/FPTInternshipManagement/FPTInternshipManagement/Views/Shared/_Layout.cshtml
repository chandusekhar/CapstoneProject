    <script>
        $(document).ready(function () {

            $('#btnRegistry').on('click', function () {
                var serviceURL = '/Home/Registry';
                var name = $('#name').val();
                var username = $('#username').val();
                var password = $('#password').val();
                var email = $('#email').val();
                var location = $('#location').val()
                $.ajax({
                    type: "POST",
                    url: serviceURL,
                    data: JSON.stringify({ name: name, username: username, password: password, email: email, location: location }),
                    contentType: "application/json; charset=utf-8",
                    dataType: "html",
                    success: successFunc,
                    error: errorFunc
                });
                function successFunc(data, status) {
                    alert("Account has been recorded! Pending review status");
                    window.location.href = window.location.href;
                }
                function errorFunc() {
                    alert('error');
                }
            })

        });
    </script>
